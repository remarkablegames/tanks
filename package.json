{
  "name": "tanks",
  "version": "0.0.1",
  "description": "Tanks is a classic 2D arcade game.",
  "author": "Mark <mark@remarkablemark.org>",
  "scripts": {
    "build": "node scripts/build",
    "clean": "rm -rf build",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "deploy": "npm run clean && npm run build && gitploy build gh-pages",
    "lint": "eslint --ignore-path .gitignore .",
    "lint:fix": "npm run lint -- --fix",
    "precommit": "lint-staged",
    "release": "standard-version --no-verify",
    "start": "node scripts/start"
  },
  "homepage": "https://remarkablegames.github.io/tanks/",
  "repository": {
    "type": "git",
    "url": "https://github.com/remarkablegames/tanks"
  },
  "bugs": {
    "url": "https://github.com/remarkablegames/tanks/issues"
  },
  "keywords": [
    "phaser",
    "template",
    "boilerplate"
  ],
  "devDependencies": {
    "@commitlint/cli": "5.2.8",
    "@commitlint/config-conventional": "5.2.3",
    "autoprefixer": "7.1.6",
    "babel-core": "6.26.0",
    "babel-eslint": "8.1.2",
    "babel-loader": "7.1.2",
    "babel-preset-env": "1.6.1",
    "babel-preset-stage-2": "6.24.1",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "css-loader": "0.28.7",
    "dotenv": "4.0.0",
    "eslint": "4.10.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-babel": "4.1.2",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-prettier": "2.4.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "fs-extra": "3.0.1",
    "gitploy": "0.1.0",
    "html-webpack-plugin": "2.29.0",
    "husky": "0.14.3",
    "lint-staged": "6.0.0",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "prettier": "1.9.2",
    "react-dev-utils": "4.2.1",
    "standard-version": "4.2.0",
    "style-loader": "0.19.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2"
  },
  "lint-staged": {
    "*.js": [
      "npm run lint:fix",
      "git add"
    ],
    "*.{json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "private": true,
  "license": "MIT"
}
